# Base cloud vagrant box needed wget (test install with non root user)
sudo yum install -y wget (should be installed by default)

# install EPEL repo for linux flavor
## RHEL/CentOS 7 64-Bit ##
sudo wget http://dl.fedoraproject.org/pub/epel/7/x86_64/e/epel-release-7-7.noarch.rpm
sudo rpm -ivh epel-release-7-7.noarch.rpm

# Install ansible
sudo yum install -y ansible

