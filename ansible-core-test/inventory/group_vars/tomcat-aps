tomcat-aps:
  monitoring:
    check_health_tomcat_aps:
      command: 'check_http -H localhost -p 8080 -u /aps/api/health'
      plugin: check_nrpe
      use: core-service
    check_load_tomcat_ams:
      command: 'check_load -w 1.5,0.8,0.8 -c 2,1,1 -r'
      plugin: check_nrpe
      use: core-service
    check_procs_tomcat_aps:
      command: 'check_procs -c 1: -C java'
      plugin: check_nrpe
      use: core-service

